<?xml version="1.0" encoding="utf-8" ?>
<robot name="crs" xmlns:xacro="http://ros.org/wiki/xacro">
  <xacro:macro name="crs_workcell" params="prefix:=^|''">
  <xacro:arg name="table_size" default="1.5"/>
  <xacro:property name="BASE_PLATE_THICKNESS" value="0.6096" scope="parent"/> CHANGED TO ACCOUNT FOR KUKA
  <xacro:property name="BASE_PLATE_LENGTH" value="0.762" scope="parent"/>
  <xacro:property name="BASE_PLATE_WIDTH" value="0.3048" scope="parent"/>

    <link name="table">
      <visual>
        <origin xyz="0 0 -0.05"/>
        <geometry>
          <box size="$(arg table_size) $(arg table_size) 0.1"/>
        </geometry>
        <material name="StainlessSteel">
          <color rgba="0.8 0.8 0.8 0.9"/>
        </material>
      </visual>
      <collision>
        <origin xyz="0 0 -0.05"/>
        <geometry>
          <box size="$(arg table_size) $(arg table_size) 0.1"/>
        </geometry>
      </collision>
    </link>

    <joint name="world_to_table_joint" type="fixed">
      <origin xyz="0 0 1.0"/>
      <parent link="world"/>
      <child link="table"/>
    </joint>

    <link name="base_plate">
      <visual>
        <origin xyz="${0.5 * BASE_PLATE_WIDTH} ${0.5 * BASE_PLATE_LENGTH} ${BASE_PLATE_THICKNESS * 0.5}"/>
        <geometry>
          <box size="${BASE_PLATE_WIDTH} ${BASE_PLATE_LENGTH} ${BASE_PLATE_THICKNESS}"/>
        </geometry>
        <material name="StainlessSteel">
          <color rgba="0.6 0.6 0.6 1.0"/>
        </material>
      </visual>
      <collision>
        <origin xyz="${0.5 * BASE_PLATE_WIDTH} ${0.5 * BASE_PLATE_LENGTH} ${BASE_PLATE_THICKNESS * 0.5}"/>
        <geometry>
          <box size="${BASE_PLATE_WIDTH} ${BASE_PLATE_LENGTH} ${BASE_PLATE_THICKNESS}"/>
        </geometry>
        <material name="StainlessSteel">
          <color rgba="0.6 0.6 0.6 1.0"/>
        </material>
      </collision>
    </link>

    <joint name="table_to_base_plate" type="fixed">
      <origin xyz="${-0.5*$(arg table_size) + 0.1397 } ${-0.5*$(arg table_size) + 0.09525} 0.0" rpy="0 0 0"/>
       <parent link="table"/>
       <child link="base_plate"/>
    </joint>
    <!-- mold -->
    <link name="layup_mold">
